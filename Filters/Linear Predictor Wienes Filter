   "source": [
    "# Wiener filter - Predictor Lineal\n",
    "N = 1000\n",
    "\n",
    "# Se√±al deseada senoidal\n",
    "n = np.arange(N)\n",
    "phi = 0.2\n",
    "d = 2*np.sin(0.1*np.pi*n + phi)\n",
    "\n",
    "# Ruido blanco\n",
    "sigv = np.sqrt(2)\n",
    "v = sigv*np.random.randn(N)\n",
    "\n",
    "x = d+v \n",
    "\n",
    "plt.figure(figsize=[12,4])\n",
    "plt.plot(x)\n",
    "plt.grid('on')\n",
    "plt.xlabel('$n$')\n",
    "plt.xlim([0,100])\n",
    "plt.title('Se√±al detectada $x[n]$')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 99,
   "id": "acting-perspective",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Error medio cuadr√°tico:  0.5220981050795543\n"
     ]
    },
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 864x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Utilizando los datos para calcular las correlaciones (Predictivo)\n",
    "\n",
    "P = 9\n",
    "\n",
    "# Matriz Rx = Rd + Rv\n",
    "rx = np.zeros(P)\n",
    "for i in range(P):\n",
    "    rx[i] = np.mean(x[0:N-i]*x[i:N])\n",
    "    \n",
    "Rx = np.zeros((P,P))\n",
    "for i in range(P):\n",
    "    for j in range(P):\n",
    "        Rx[i,j] = rx[np.abs(i-j)]\n",
    "\n",
    "# Vector rdx = rd\n",
    "rdx = np.zeros(P)\n",
    "for i in range(P):\n",
    "    rdx[i] = np.mean(x[0:N-(i+1)]*x[(i+1):N])\n",
    "\n",
    "# Calculando Filtro Wiener\n",
    "W1 = np.dot(np.linalg.inv(Rx),rdx)\n",
    "dW1 = signal.lfilter(W1,[1],x)\n",
    "\n",
    "plt.figure(figsize=[12,4])\n",
    "plt.plot(dW1)\n",
    "plt.plot(x)\n",
    "\n",
    "plt.grid('on')\n",
    "plt.xlabel('$n$')\n",
    "plt.xlim([0,100])\n",
    "plt.title('Se√±al estimada $\\hat{d}[n]$')\n",
    "plt.legend(['Predictor Lineal','$x[n]$'])\n",
    "\n",
    "print('Error medio cuadr√°tico: ',np.mean((d-dW1)**2))\n"
   ]
  },
